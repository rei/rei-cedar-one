{
  "name": "@rei/c1-icons",
  "version": "0.1.1-alpha.0",
  "private": true,
  "description": "Framework-agnostic Cedar icon assets (sprite, raw SVGs, JSON, JS).",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./icons.json": "./dist/icons.json",
    "./icon-meta.json": "./dist/icon-meta.json",
    "./icon-names.json": "./dist/icon-names.json",
    "./sprite.svg": "./dist/sprite.svg",
    "./icons/*": "./dist/icons/*.svg"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run validate && tsx scripts/build.ts",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "tsx scripts/validate.ts"
  },
  "devDependencies": {
    "@rei/c1-lint": "workspace:*",
    "@types/node": "^24.10.1",
    "svgo": "^3.3.2",
    "svgstore": "^3.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  }
}
