{
  "name": "@rei/c1-icons",
  "version": "0.1.3-alpha.0",
  "private": true,
  "description": "Framework-agnostic Cedar icon assets (sprite, raw SVGs, JSON, JS).",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js"
    },
    "./vue/*": "./dist/vue/icons/*.vue",
    "./vue/icon": "./dist/vue/C1Icon.vue",
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./react/*": "./dist/react/icons/*.js",
    "./react/icon": {
      "types": "./dist/react/C1Icon.d.ts",
      "import": "./dist/react/C1Icon.js"
    },
    "./data.json": "./dist/icons.json",
    "./meta.json": "./dist/icon-meta.json",
    "./names.json": "./dist/icon-names.json",
    "./sprite.svg": "./dist/sprite.svg",
    "./svg/*": "./dist/icons/*.svg"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run validate && tsx scripts/build.ts",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit && vue-tsc -p tsconfig.vue.json --noEmit",
    "validate": "tsx scripts/validate.ts"
  },
  "devDependencies": {
    "@rei/c1-lint": "workspace:*",
    "@types/node": "^24.10.1",
    "svgo": "^3.3.2",
    "svgstore": "^3.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "vue": "^3.5.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
