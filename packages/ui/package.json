{
  "name": "@rei/c1-ui",
  "version": "0.1.10-alpha.0",
  "private": true,
  "sideEffects": [
    "./dist/css/**"
  ],
  "exports": {
    "./fonts.css": "./dist/css/fonts.css",
    "./reset.css": "./dist/css/reset.css",
    "./components/*": "./dist/css/components/*"
  },
  "scripts": {
    "build": "postcss \"src/css/**/*.css\" --base src/css --dir dist/css",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@rei/c1-tokens": "workspace:*"
  },
  "devDependencies": {
    "@rei/c1-lint": "workspace:*",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-custom-media": "^10.0.4",
    "postcss-import": "^16.1.0"
  }
}
