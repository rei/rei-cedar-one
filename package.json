{
  "name": "rei-cedar-unified-lab",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "format": "prettier --write --single-quote",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "typecheck": "pnpm -r exec tsc -b"
  },
  "lint-staged": {
    "package.json": "pnpm dlx sort-package-json@latest",
    "*.{ts,tsx,mts,cts,js,jsx,mjs,cjs,json,md,mdx,vue,yml,yaml}": [
      "pnpm format",
      "pnpm lint:fix"
    ]
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "typescript": "^5.6.3",
    "typescript-eslint": "latest"
  }
}
